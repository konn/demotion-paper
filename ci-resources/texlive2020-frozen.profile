# texlive2020-frozen.profile
#
selected_scheme scheme-minimal
collection-langjapanese 1
collection-latexextra 1
collection-luatex 1
collection-fontsrecommended 1
collection-fontsextra 1
collection-mathscience 1
collection-plaingeneric 1
#
##
TEXDIR /opt/texlive/2020
TEXMFLOCAL /opt/texlive/texmf-local
TEXMFSYSCONFIG /opt/texlive/2020/texmf-config
TEXMFSYSVAR /opt/texlive/2020/texmf-var
TEXMFHOME ~/texmf
TEXMFCONFIG ~/.texlive2020/texmf-config
TEXMFVAR ~/.texlive2020/texmf-var
#
## set platform(s)
binary_x86_64-darwin 0
binary_x86_64-linux 1
binary_win32 0
#
## texmf-dist/doc, texmf-dist/src
option_doc 0
option_src 0
#
## set other options
option_adjustrepo 0
option_autobackup 0
option_backupdir tlpkg/backups
option_desktop_integration 0
option_file_assocs 0
option_fmt 1
option_letter 0
option_path 0
option_post_code 1
option_sys_bin /usr/local/bin
option_sys_info /usr/local/share/info
option_sys_man /usr/local/share/man
option_w32_multi_user 1
option_write18_restricted 1
portable 0
